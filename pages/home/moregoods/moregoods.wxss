}
.body{
  width: 100%;
  height: 100%;
}
.header{
  width: 100%;
  text-align: center;
}
.right{
  width: 100%;
  /* height: 394px; */
  float: left;
  /* margin-left: 1%; */
}
.right view{
  width: 40%;
  /* height: 118px; */
  float: left;
  margin-left:6.5%;
  margin-top:5%;
}
.right view>image{
  width: 300rpx;
  height: 360rpx;
  display: block;
  float: left;
}
.nav {
  width: 30%;
  height: 160rpx;
  float: left;
  font-size: 18px;
}
.maddle{
  width: 100%;
  height: 100%;
  /* margin-top:5%; */
  border-top:1px solid #f2f2f2;
}
#asd{
  width: 100%;
  float:left;
  margin-left:0%;
  margin-top:4%;
  display: block;
}
#asd image{
  width: 20px;
  height: 20px;
  display: inline-block;
  margin-left: 50%;
  margin-top: 1%;
}
#asd text:nth-child(1){
  font-size: 30rpx;
  display: block;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
#asd text:nth-child(2){
  /* display: inline-block; */
  font-size: 30rpx;
  /* display: inline-block; */
  float: left;
}