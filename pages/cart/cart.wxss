page{
    background-color: #ffffff;
    font-family: "Helvetica Neue","Hiragino Sans GB","Microsoft YaHei","\9ED1\4F53",Arial,sans-serif;
    font-size: 32rpx;
}
.navigator-hover{
    background: none;
}
 
.cart-box{
    padding-bottom: 100rpx;
} 
.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 285rpx;
    height: 220rpx;
    margin-top: 20rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .cart-pro-select{
    position: absolute;
    left: 20rpx;
    top: 90rpx;
    width: 45rpx;
    height: 45rpx;
    border-radius: 50%;
}

checkbox .wx-checkbox-input {
  border-radius: 50%;
}

.cart-list .cart-thumb{
    position: absolute;
    top: 20rpx;
    left: 85rpx;
    width: 212rpx;
    height: 220rpx;
    
}
/* 商品名字规格价格 */
.cart-list .cart-pro-name{
    display: inline-block;
    width: 400rpx;
    margin-left: 16px;
    line-height: 46rpx;
    overflow: hidden;
}
.cart-list .cart-pro-format{
    position: absolute;
    top:30px;
    left: 173px;
    width: 400rpx;
    line-height: 46rpx;
    overflow: hidden;
    float:left;
    margin-top:3%;
}
.cart-list .cart-pro-price{
    position: absolute;
    line-height: 50rpx;
    bottom: 0px;
    left:41%;
 

}
/* 数量加减操作 */
.cart-list .cart-count-box{
    position: absolute;
    left: 67%;
    top:58%;
    bottom: 20rpx;
    /* width: 228rpx; */
    height: 66rpx;
    border:1px solid #f2f2f2;
    border-radius:30px;
    padding-top: 14rpx;
}
.cart-list .cart-count-box text{
    display: inline-block;
     font-size: 40rpx;
     height: 40rpx;
    line-height: 40rpx;
    text-align: center;
}
.cart-count-down,.cart-count-delete,.cart-count-add{
  width: 50rpx;
}

.cart-count-num{
  width:56rpx;
  border-left:1px solid #000;
  border-right:1px solid #000;
}
.cart-del{
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 44rpx;
}
/* 底部汇总操作 */
.cart-footer{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    background-color: #fff;
    color: #000;
    border-bottom:1px solid #392315;
    border-top:1px solid #f2f2f2;

}
.total-select{
    position: absolute;
    left: 20rpx;
    top: 25rpx;
    width: 45rpx;
    height: 45rpx;
}
.order-icon{
  position: absolute;
  right:0;
  width:80px;
  height:49px;
  background-color:#392315;
  text-align:center;
}
.order-icon text{
  width: 30px;
  height: 100%;
  color: #fff;
  background-color: #392315;
}


.order-icon image,.order-icon navigator{
  display: block;
  width: 100%;
  height: 45rpx;
}
/* .cart-toatl-price{
    margin-left: 33%;
    width: 120rpx;
} */
.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}
/* 没有商品时页面样式 */
.cart-no-data{
  width: 150px;
  height: 150px;
  margin: 0 auto;
}
.cart-no-data image{
  width: 100%;
  height: 100%;
}
.cart-no-data view{
  width: 100%;
  height: 150px;
}

@media only screen and (max-width: 320px){ 
  .cart-list .cart-pro-price{
    top:62%;
    left:42%;
  }
  .cart-list .cart-count-box{
    top:55%;
    width:35%;
    left: 64% !important;
  }
  .cart-count-num{
    width:30%;
  }
}
@media only screen and (max-width: 375px){ 
  .cart-list .cart-pro-format{
    left:42%;

  }
  .cart-toatl-price{
    margin-left:27%;
  }
  .cart-list .cart-count-box{
    left:69%;
  }
}
