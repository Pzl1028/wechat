@import'../../style/style.wxss';
.header{
  width: 100%;
  height: 25%;
  padding-top: 5%;
  background-color: #ccb2a3;
}
.header view{
  width:100%;
  height:30px;
  text-align:left;
  text-indent:1em;
  font-size:16px;
  margin-bottom:8%;
}
.header text{
  width:20%;
  height:35px;
  margin:0 auto;
  text-align:center;
  font-size:1.5em;
}
.maddle{
  width: 100%;
  height:30px;
  margin-top: 2%;
  padding-bottom: 1%;
}
.maddle view{
  width: 38%;
  height: 100%;
  text-align:left;
  text-indent:1em;
  /* background:red; */
  float:left;
  line-height: 30px;
}
.maddle text{
  width:30%;
  height:100%;
  float:right;
  text-align:right;
  margin-right:4%;
}

.footer{
  width: 100%;
  height: 59%;
  padding-top: 5%;
  background: #f8f8f8;
}
.footer view{
  text-align:left;
  text-indent:1em;
}
.footer text{
  text-indent:1em;
  font-size:14px;
  margin-top:3%;
}