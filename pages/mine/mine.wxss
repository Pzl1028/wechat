}
.huihua{
  width:180rpx;
  padding-top:23%;
  margin-top:13%;
  background:#fff;
}
.huihua::after{
  border: none;
}
/* 个人中心选项 */
.footer{
  width: 100%;
  /* height: 460rpx; */
  margin: 0 auto;
  background-color: #fff;
  margin-top: 2%;
}
.xuangxiangk{
  width: 100%;
  height: 100rpx;
  border-bottom:1px solid #e8e8e8;
}
.xuangxiangk text{
  height: 50px;
  line-height: 56px;
  /* float: left; */
  margin-left: 4.5%;
  width: 40%;
  display: inline-block;
}
.tubiao{
  text-align: right;
}
.xuangxiangk image{
  width: 16px;
  height: 16px;
  float: right;
  margin-right:5%;
  margin-top:4%;
}
.xuangxiangk navigator{
  display: block;
  width: 100%;
  height: 100%;
}
  /* 不同手机兼容 */
@media only screen and (max-width: 320px){ 
  #headerl{
    position:absolute;
    top:28rpx;
  }
  .headerma{
    font-size:30rpx;
    position:absolute;
    left:190rpx;
    top:16rpx;
  }
  .wxtxd open-data{
    width: 60px;
    height: 60px;
    display: block;
    overflow: hidden;
    border-radius:50%;
  }
  .wxtxd{
    display: block;
    width: 60px !important;
    height: 60px !important;
    float: left !important;
    margin-top: 10% !important;
  }
  .headerma{
    width: 32%;
  }
  #headerl view{
    margin-top: 10% !important;
    margin-bottom: 2%;
    margin-left: 5%;
  }
  #headerl text{
    font-size: 12px;
    margin-left: 5%;
  }
  .headerr{
    width: 42% !important;
  }
  .fahuxl{
    height:168rpx !important;
  }
  .headerr text{
    font-size: 13px !important;
    
  }
  .xuangxiangk text{
    height: 42px;
    line-height: 42px;
    clear: both ;

  }
  /* .xuangxiangk image{

