  float: left;
}
.headerb text{
  color: #cfb5a4;
  float: left;
  margin-top: 1%;
}
.headerb label{
  font-size: 14px;
  float: left;
  line-height: 30px;
}
.headerb input{
  width:60%;
  height: 30px;
  float: left;
  font-size: 12px;
