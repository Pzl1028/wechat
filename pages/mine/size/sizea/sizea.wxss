@import'../../../style/style.wxss';
.header{
  width: 100%;
  height: 40%;
  padding-top: 2%;
  /* background-color: rgb(240, 252, 251); */
}
.headera{
  width: 97%;
  height: 30px;
  margin-left: 3%;
}
.headera text{
  color: #cfb5a4;
  float: left;
  margin-top: 1%;
}
.headera label{
  font-size: 14px;
  float: left;
  line-height: 30px;
}
.headera input{
  width: 78%;
  height: 30px;
  float: left;
  font-size:14px;
  margin-left: 5.2%;
  border: 1px solid #dadada;
}

.headerb{
  width:45.6%;
  height: 30px;
  margin-left: 3%;
