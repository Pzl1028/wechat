<view class='body'>
  <view class='moren' style='display:{{asdasd}};'>
    <image src='/img/isok.png' class='asda' ></image>
    <text class='asda' >默认地址</text>
  </view>
  <block  wx:for="{{address}}" wx:key="item.address_id">
    <view class='header'>
       <view class='headera'><text>{{item.consignee}}</text> <text>{{item.phone}}</text> </view>
        <text class='headerc'>{{item.area}}{{item.detail}}</text>
         <view class='headerb'>
          <image src='/img/isok.png' class='asd' style='display:{{none}}none'></image>
          <text class='asd' style='display:{{none}}none'>默认地址</text>
          <image id='{{item.address_id}}' bindtap='sumit' src='/img/scdz.png'></image>
          <navigator url='/pages/mine/modifyaddres/modifyaddres?address_id={{item.address_id}}&consignee={{item.consignee}}&area={{item.area}}&phone={{item.phone}}&detail={{item.detail}}'>
          <image id='{{item.address_id}}'src='/img/bjdz.png'></image></navigator>
        </view>
    </view>    
  </block>
  <text class='asd' style='display:{{none}}none'>默认地址</text>
  <view style='width:100%;height:60rpx;'></view>
  <navigator url='/pages/mine/address/address'>
  <view class='footer'>添加地址</view></navigator>

</view>